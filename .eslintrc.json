{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      // Customize any rules here
      "no-console": "off",
      "indent": ["error", 2],
      "max-len": ["error", { "code": 120 }]
    }
  }
  